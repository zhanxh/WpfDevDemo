<Window x:Class="WpfControlDev.View.WinVersionCmp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfControlDev.View"
        mc:Ignorable="d"
        Title="版本比较" Height="600" Width="800">
    <Grid>
        <Button x:Name="btn1" Content="目录1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="btn1_Click"/>
        <Button x:Name="btn2" Content="目录2" HorizontalAlignment="Left" Margin="0,22,0,0" VerticalAlignment="Top" Width="75" Click="btn2_Click"/>
        <TextBlock x:Name="txt1" HorizontalAlignment="Left" Margin="80,0,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="633" Height="22"/>
        <TextBlock x:Name="txt2" HorizontalAlignment="Left" Margin="80,22,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="633" Height="22"/>
        <ScrollViewer Margin="0,49,0,0">
            <TextBox x:Name="txtCmp" TextWrapping="Wrap" Text="TextBox" />
        </ScrollViewer>
        <Button x:Name="start" Content="开始" HorizontalAlignment="Left" Margin="718,22,-1,0" VerticalAlignment="Top" Width="75" Click="start_Click"/>
    </Grid>
</Window>
