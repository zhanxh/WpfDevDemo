<Window x:Class="WpfControlDev.View.WinCodeSeach"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfControlDev.UC"
        mc:Ignorable="d" PreviewKeyDown="Window_PreviewKeyDown"
        Title="MainWindow" Height="350" Width="525">
    <Grid Name="searchGrid">
        <local:UCCodeSeach Visibility="Hidden" HorizontalAlignment="Right" Margin="0,0,10,10" x:Name="codesearch1" VerticalAlignment="Bottom" Height="240" Width="160" />
        <TextBox Height="25" Text="{Binding TxtData}" Width="64" Margin="10,285,443,10"></TextBox>
    </Grid>
</Window>
